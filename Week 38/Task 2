/*               2  .  C
 void setup () {
 
 println (upperCaser("hest"));
 }
 
 
 String upperCaser(String lowerCase){
 
 String newLine = lowerCase.toUpperCase();  
 
 return (newLine);
 }
 
 
 */

void setup () {
  String tester = "tester";
  capLetter(tester);

  if (capLetter(tester)==true) {
    println("true");
  } else {
    println("false");
  }
}

boolean capLetter (String test) {
  if (Character.isUpperCase(test.charAt(0))) {
    return true;
  } else {

    return false;
  }
}
