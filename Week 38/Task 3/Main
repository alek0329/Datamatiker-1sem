Teacher teacher;
Student student1;
Student student2;

void setup() {
  
  teacher = new Teacher(35, "Jesper",false);
  student1 = new Student (23, "Oliver", false, 'B');
  student2 = new Student (23, "Aleksander", false, 'B');
  
  println("Teacher: " + teacher.name);
  println("Student: " + student1.name + " \thold: " + student1.datamatikerTeam);
  println("Student: " + student2.name + " \thold: " + student2.datamatikerTeam);
}
